/* -------------------------BASE TEESPRING CSS CODE START HERE--------------------------------------*/

.js-qty {clear: both;}

.select-label-bs-tee input {
  margin-bottom:15px;
}

.select-label-bs-tee label {
  font-weight: bold;
  display: block;
}

.breadcrumb-bs-tee  a:first-child, .breadcrumb-bs-tee  span:first-child {
  padding-left: 0;
 }

.breadcrumb-bs-tee .divider-bs-tee { 
 padding: 10px 7px 10px 0; 
 }

.breadcrumb-bs-tee a, .breadcrumb-bs-tee span {
 display: inline-block;
 padding: 10px 7px 10px 0;
 margin-right: 7px;
 overflow: hidden;
 }

 .breadcrumb-bs-tee {
  width: 100%;
  margin-bottom: 15px !important;  
 }


.select-label-bs-tee .selector-wrapper{ border:none;}

.select-label-bs-tee .selector-wrapper select {
  max-width: 100% !important;
  width: 100% !important;
  margin: 0% 0% 5% 0%;
  border:1px solid  ;
    
 }

.select-label-bs-tee .selector-wrapper label{
 font-weight: bold;
 }

.grid-bs-tee .large_2_5-bs-tee form {
 margin-bottom: 0px;
 }

.add-to-cart-btn-bs-tee { 
 padding: 20px 20px;
 width: 100%;
 margin: 0% 0% 5% 0%;
 }

.add-to-cart-btn-bs-tee .icon-cart {
 margin: 0px;
 padding: 0px;
 }

.product-title-bs-tee {
 font-size: 2rem;
 text-transform: capitalize;
 }

.grid-bs-tee { 
 list-style: none;
 margin: 0;
 margin-left: -30px;
 padding: 0px;
 }

.grid-bs-tee:after { 
 content: "";
 display: table;
 clear: both;
 }

.grid-uniform-bs-tee:after {
 content: "";
 display: table;
 clear: both;
 }

.grid-item-bs-tee {
 float: left;
 padding-left: 30px;
 vertical-align: top;
 width: 100%;
 -webkit-box-sizing: border-box;
 -moz-box-sizing: border-box;
 box-sizing: border-box;
 }

.grid-item-bs-tee img {
  max-width: 100%;
 }

.large_3_5-bs-tee {
 width: 60%; 
 }

.large_11-12-bs-tee {
 width: 91.666%;
 }

.text-center-bs-tee {
 text-align: center !important;
 }

.grid-uniform-bs-tee {
 list-style: none;
 margin: 0;
 padding: 0;
 margin-left: -30px;
 }

.large_1_6-bs-tee {
 width: 16.666%; 
 }

.large_1_whole-bs-tee {  
 width: 100%;
 }
  
.product-photo-container-bs-tee {
 margin-bottom: 30px;
 max-width: 100%;
 }

.large_2_5-bs-tee {
 width: 40%;
 }
  
.grid-bs-tee .large_2_5-bs-tee {  
 float: right;
 }

.grid-bs-tee .large_3_5-bs-tee .large_1_6-bs-tee {  
 float: left;
 margin: 0px;
 }

.large_5_6-bs-tee {
 width: 83.333%;
 }

.grid-bs-tee .large_3_5-bs-tee .large_5_6-bs-tee {  
 float: right;
 }

.grid-bs-tee ul li {
 list-style: none;
 }

.large_3_5-bs-tee .large_1_6-bs-tee li {
 margin-bottom: 10px;
 padding-left: 0px;    
 border: 1px solid;
 border-radius: 2px;
 padding: 4px 2px 0px 2px;
 clear: both;  
 width: 50%;
 margin: 5%;
 }

.large_3_5-bs-tee .large_1_6-bs-tee li:hover { 
 opacity: 0.5;
 }

.large_3_5-bs-tee .large_1_6-bs-tee li a { 
 display: block;
 margin: 0 auto;
 height: auto;
 }

.grid-bs-tee .large_3_5-bs-tee .large_5_6-bs-tee img {
 display: block;
 margin: 0 auto;
 }

.product-meta-bs-tee {
 padding-left: 0px;
 margin-left: 0px;
 margin-bottom: 5px;
 }

.product-meta-bs-tee sup{
 top: -0.5em;
 position: relative;
}

.rte-bs-tee {
 margin-bottom: 15px;
 }

.rte-bs-tee>div {
 margin-bottom: 15px;
 }

.large_1_whole-bs-tee img {
 max-width: 100%;
 width: 100%;
 }

.social-sharing-bs-tee img {
 width: 30px;
 background-size: 100%; 
 }

.social-sharing-bs-tee {  
 padding-top: 15px;
 }

.social-sharing-bs-tee a {
 display: inline-block; 
 border-radius: none;
 margin: 0 8px 10px 0;  
 line-height: 30px;
 height: 30px;
 text-decoration: none;  
 }

.social-sharing-bs-tee span {
 display: inline-block;
 vertical-align: top;
 height: 30px;
 line-height: 30px; 
 }

.social-sharing-bs-tee .icon-bs-tee {
 padding: 0px;
 }


/* Code For Responsive (SH)*/

@media screen and (max-width: 767px) {
.grid-bs-tee .large_3_5-bs-tee {
 width: 100%;
 }
.grid-bs-tee .large_2_5-bs-tee {
 width: 100%;
 }
.large_2_5-bs-tee .social-sharing-bs-tee {
 text-align: center;
 }  
}
/* Code For Responsive (EH)*/



